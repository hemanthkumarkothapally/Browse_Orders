<mvc:View xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.f"
    controllerName="com.db.oders.orderdetails.controller.OrderDetails"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
>
    <f:FlexibleColumnLayout id="flexibleColumnLayout">
        <f:beginColumnPages>
            <f:DynamicPage>
                <f:title>
                    <f:DynamicPageTitle>
                        <f:heading>
                            <Title id="maintittle" text="Orders ({= ${orderdetailsmodel>/totalorders}})" />
                        </f:heading>
                    </f:DynamicPageTitle>
                </f:title>
                <f:header>
                    <f:DynamicPageHeader>
                        <f:content>
                            <OverflowToolbar
                                class="sapFDynamicPageAlignContent"
                            >
                                <SearchField
                                    search=".onSearch"
                                    width="16rem"
                                />
                                <ToolbarSpacer />
                                <OverflowToolbarButton
                                    icon="sap-icon://filter"
                                    text="Add"
                                    type="Transparent"
                                    press=".onFilter"
                                />
                                <OverflowToolbarButton
                                    icon="sap-icon://group-2"
                                    text="Sort"
                                    type="Transparent"
                                    press=".onGroup"
                                />
                            </OverflowToolbar>
                        </f:content>
                    </f:DynamicPageHeader>
                </f:header>
                <f:content>
                    <List
                        id="orderList"
                        items="{
    path: '/Orders',
    parameters: {
      expand: 'Customer'
    }
  }"
                    >
                        <items>
                            <CustomListItem
                                type="Navigation"
                                press="onlistitempress"
                            >
                                <HBox
                                    class="listcolumn"
                                    justifyContent="SpaceBetween"
                                    width="100%"
                                >
                                    <!-- Left Column: ID, Name, Date -->
                                    <VBox justifyContent="Center">
                                        <Title text="Order {OrderID}" />
                                        <Text text="{Customer/ContactName}" />
                                        <Text
                                            text="Shipped: { path: 'ShippedDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'long' } }"
                                        />
                                        <!-- You can adjust this if date is not available -->
                                    </VBox>

                                    <!-- Right Column: Price -->
                                    <VBox class="sapUiSmallMarginEnd">
                                        <Title
                                            text="{ path: 'OrderDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short' } }"
                                            textAlign="End"
                                            level="H6"
                                        />
                                        <ObjectStatus
                                            state="{
									parts: [
										{path: 'RequiredDate'},
										{path: 'ShippedDate'}									],
									formatter:'.deliveryState'
								}"
                                            text="{
									parts: [
										{path: 'RequiredDate'},
										{path: 'ShippedDate'}
									],
									formatter:'.deliveryText'
								}"
                                        />
                                    </VBox>
                                </HBox>
                            </CustomListItem>
                        </items>
                    </List>
                </f:content>
            </f:DynamicPage>
        </f:beginColumnPages>
        <f:midColumnPages>
            <f:DynamicPage>
                <f:title>
                    <f:DynamicPageTitle>
                        <f:heading>
                            <Title
                                text="Order({= ${orderdetailsmodel>/selectedorder/OrderID}}) "
                            />
                        </f:heading>
                        <f:navigationActions>
                            <Button
                                id="expandbutton"
                                icon="sap-icon://full-screen"
                                type="Transparent"
                                press="onexpanddetailspage"
                            />
                            <Button
                                icon="sap-icon://decline"
                                type="Transparent"
                                press="onclosedetailspage"
                            />
                        </f:navigationActions>
                    </f:DynamicPageTitle>
                </f:title>
                <f:header>
                    <f:DynamicPageHeader pinnable="true">
                        <layout:HorizontalLayout allowWrapping="true">
                            <layout:VerticalLayout class="sapUiMediumMarginEnd">
                                <ObjectAttribute
                                    title="Customer"
                                    text="{orderdetailsmodel>/selectedcustomer/ContactName}"
                                />
                                <ObjectAttribute
                                    title="Ordered"
                                    text="{path:'orderdetailsmodel>/selectedorder/RequiredDate', formatter:'.formatdate'}"
                                />
                                <ObjectAttribute
                                    title="Shipped"
                                    text="{path:'orderdetailsmodel>/selectedorder/ShippedDate',formatter:'.formatdate'}"
                                />
                            </layout:VerticalLayout>

                            <layout:VerticalLayout>
                                <ObjectAttribute title="Price" />
                                <ObjectNumber
                                    id="someTextId"
                                    number=""
                                    unit="EUR"
                                />
                            </layout:VerticalLayout>
                        </layout:HorizontalLayout>
                    </f:DynamicPageHeader>
                </f:header>
                <f:content>
                    <VBox>
                        <IconTabBar
                            id="icontabbar"
                            headerBackgroundDesign="Transparent"
                            selectedKey="shipping"
                        >
                            <items>
                                <IconTabFilter
                                    icon="sap-icon://shipping-status"
                                    key="shipping"
                                >
                                    <form:SimpleForm
                                        editable="false"
                                        layout="ResponsiveGridLayout"
                                        title="Shipping Address"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                        emptySpanXL="4"
                                        emptySpanL="4"
                                        emptySpanM="4"
                                        emptySpanS="0"
                                        columnsXL="1"
                                        columnsL="1"
                                        columnsM="1"
                                        singleContainerFullSize="false"
                                    >
                                        <form:content>
                                            <Label text="Name" />
                                            <Text
                                                id="Name"
                                                text="{orderdetailsmodel>/selectedorder/ShipName}"
                                            />
                                            <Label text="Street" />
                                            <Text
                                                text="{orderdetailsmodel>/selectedorder/ShipAddress}"
                                            />
                                            <Label text="ZIP Code/City" />
                                            <Text
                                                text="{orderdetailsmodel>/selectedorder/ShipName} {orderdetailsmodel>/selectedorder/ShipCity}"
                                            />
                                            <!-- <Label text="Region" />
                                        <Text
                                            
                                            text="{orderdetailsmodel>/selectedorder/ShipRegion}"
                                        /> -->
                                            <Label text="Country" />
                                            <Text
                                                id="countryText"
                                                text="{orderdetailsmodel>/selectedorder/ShipCountry}"
                                            />
                                        </form:content>
                                    </form:SimpleForm>
                                </IconTabFilter>
                                <IconTabFilter
                                    icon="sap-icon://person-placeholder"
                                    key="personal"
                                >
                                    <form:SimpleForm
                                        editable="false"
                                        layout="ResponsiveGridLayout"
                                        title="Processor Information"
                                        labelSpanXL="4"
                                        labelSpanL="4"
                                        labelSpanM="12"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                        emptySpanXL="0"
                                        emptySpanL="0"
                                        emptySpanM="0"
                                        emptySpanS="0"
                                        columnsXL="2"
                                        columnsL="2"
                                        columnsM="2"
                                        singleContainerFullSize="false"
                                    >
                                        <form:content>
                                        <core:Title text="Details"></core:Title>
                                            <Label text="Name" />
                                            <Text
                                                text="{= ${orderdetailsmodel>/selectedEmployee/FirstName}+${orderdetailsmodel>/selectedEmployee/LastName}}"
                                            />
                                            <Label text="Employee ID" />
                                            <Text
                                                text="{orderdetailsmodel>/selectedEmployee/EmployeeID}"
                                            />
                                            <Label text="Job Title" />
                                            <Text
                                                text="{orderdetailsmodel>/selectedEmployee/Title}"
                                            />
                                            <Label text="Phone" />
                                            <Text
                                                text="{orderdetailsmodel>/selectedEmployee/HomePhone}"
                                            />
                                            <core:Title text="Picture" />
                                            <!-- <Image
                                                src="data:image/png;base64,"
                                            /> -->
                                            <Image
                                                src="{path: 'orderdetailsmodel>/selectedEmployee/Photo', formatter: '.formatPhoto'}"
                                                width="10rem"
                                                height="200px"
                                            />
                                        </form:content>
                                    </form:SimpleForm>
                                </IconTabFilter>
                            </items>
                        </IconTabBar>
                        <Table
                            id="productstable"
                            items="{
                                path: '/Order_Details',
                                parameters: {
                                expand: 'Product'
                                }}"
                        >
                            <headerToolbar>
                                <Toolbar>
                                    <Title
                                        text="Products Ordered ({= ${orderdetailsmodel>/totalproductssum}})"
                                        level="H2"
                                    />
                                </Toolbar>
                            </headerToolbar>
                            <columns>
                                <Column>
                                    <Text text="Product" />
                                </Column>
                                <Column>
                                    <Text text="Unit Price" />
                                </Column>
                                <Column>
                                    <Text text="Quantity" />
                                </Column>
                                <Column>
                                    <Text text="Discount" />
                                </Column>
                                <Column>
                                    <Text text="Total" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <ObjectIdentifier
                                            title="{Product/ProductName}"
                                            text="{ProductID}"
                                        />
                                        <ObjectNumber
                                            number="{path: 'UnitPrice', formatter: '.decimalPrice'}"
                                            unit=" EUR"
                                        />
                                        <Text text="{Quantity}" />
                                        <Text text="{= ${Discount}*100} %" />
                                        <ObjectNumber
                                            number="{parts: [{path: 'UnitPrice'},{path: 'Quantity'},{path: 'Discount'},{path: 'OrderID'}], formatter:'.totalcaliucate'}"
                                            unit=" EUR"
                                        />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </VBox>
                </f:content>
            </f:DynamicPage>
        </f:midColumnPages>
    </f:FlexibleColumnLayout>
</mvc:View>
